find_last_high_volume(data, n) =>
    r = 0
    for i=0 to n
        if data[i] > vol_per_trigger
            r := i 
            break
    r

get_price_data(period) =>
    v = 100 * (volume / ta.highest(volume, period))
    f = find_last_high_volume(v, period)
    src = open[f] > close[f] ? low : high
    [v, f, src]

