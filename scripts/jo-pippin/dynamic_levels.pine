f_avwap(src, date) =>
    start  = time >= date and time[1] < date
    sumSrc = src * volume
    sumVol = volume
    sumSrc := start ? sumSrc : sumSrc + sumSrc[1]
    sumVol := start ? sumVol : sumVol + sumVol[1]
    sumSrc / sumVol

